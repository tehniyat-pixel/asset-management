{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.js"
    }
  ]
  "rewrites": [
    { "source": "/admin", "destination": "/api/admin" },
    { "source": "/api/assets", "destination": "/api/assets" }
  ]
}
